{
    "parser": "babel-eslint",
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-single"
      ],
      "react/jsx-closing-bracket-location": [
        1,
        "tag-aligned"
      ],
      "no-duplicate-imports": "error",
      "react/jsx-max-props-per-line": [
        1,
        {
          "maximum": 1,
          "when": "always"
        }
      ],
      "react/jsx-first-prop-new-line": [
        1,
        "multiline-multiprop"
      ],
      "quotes": [
        2,
        "single"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-param-reassign": "error",
      "semi": 2,
      "sort-imports": [
        2,
        {
          "ignoreCase": true,
          "memberSyntaxSortOrder": [
            "single",
            "multiple",
            "all",
            "none"
          ]
        }
      ],
      "operator-linebreak": "off",
      "react/display-name": "off",
      "func-names": "off",
      "max-len": [
        "warn",
        {
          "code": 140
        }
      ],
      "prefer-destructuring": 0,
      "arrow-parens": 0,
      "import/newline-after-import": 0,
      "no-prototype-builtins": 0,
      "lines-around-directive": 0,
      "no-unsafe-negation": 0,
      "import/first": 0,
      "import/order": 0,
      "import/no-unresolved": 0,
      "import/no-webpack-loader-syntax": 0,
      "prefer-spread": 0,
      "no-underscore-dangle": 0,
      "no-template-curly-in-string": 0,
      "class-methods-use-this": 0,
      "no-confusing-arrow": 0,
      "no-mixed-operators": 0,
      "no-undef-init": 0,
      "no-plusplus": 0,
      "linebreak-style": "off",
      "import/prefer-default-export": 0,
      "no-useless-escape": 0,
      "no-loop-func": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "no-lonely-if": 0,
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "no-extra-boolean-cast": 0,
      "no-multi-assign": 0,
      "newline-per-chained-call": 0,
      "block-spacing": 0,
      "object-property-newline": 0,
      "no-useless-rename": 0,
      "no-restricted-syntax": 0,
      "no-useless-return": 0,
      "no-continue": 0,
      "no-console": 2,
      "no-debugger": 2,
      "function-paren-newline": 0,
      "object-curly-newline": 0,
      "arrow-body-style": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "react/sort-comp": 2,
      "no-await-in-loop": "off",
      "react/jsx-no-bind": [
        2,
        {
          "allowArrowFunctions": true,
          "allowBind": false
        }
      ],
      "id-blacklist": [
        "error",
        "err",
        "e",
        "cb",
        "callback",
        "handleChange",
        "changeValue"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "jest/globals": true
    },
    "globals": {
      "shallow": true,
      "mount": true,
      "BigInt": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "airbnb/base",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "react",
      "import",
      "jest"
    ]
  }
  